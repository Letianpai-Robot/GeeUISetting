<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GeeUISetting" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="update_progress_horizontal" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:indeterminateOnly">false</item>
        <!--进度条的进度颜色drawable文件-->
        <item name="android:progressDrawable">@drawable/seekbar</item>
        <!--进度条的最小高度-->
        <item name="android:minHeight">73px</item>
        <!--进度条的最大高度-->
        <item name="android:maxHeight">73px</item>
    </style>

    <style name="SwitchTheme" parent="Theme.AppCompat.Light">
        <item name="android:textColor">@color/btn_bg</item>
    </style>

    <style name="SwitchThemeOff" parent="Theme.AppCompat.Light">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="DeviceInfoText">
        <item name="android:textSize">16px</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5px</item>
    </style>

</resources>